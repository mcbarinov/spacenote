{
  "name": "@spacenote/common",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "prettier": "./configs/prettier.json",
  "description": "Common frontend code: API layer, components, utilities, types",
  "exports": {
    "./api": "./src/api/index.ts",
    "./app": "./src/app/index.ts",
    "./components": "./src/components/index.ts",
    "./errors": "./src/errors/index.ts",
    "./templates": "./src/templates/index.ts",
    "./styles/templates.css": "./src/styles/templates.css",
    "./types": "./src/types/index.ts",
    "./utils": "./src/utils/index.ts",
    "./configs/*": "./configs/*"
  },
  "scripts": {
    "generate": "openapi-typescript http://localhost:3100/openapi.json -o src/types/openapi.gen.ts --export-type",
    "format": "prettier --write . | grep -v '(unchanged)' || true",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "peerDependencies": {
    "@tanstack/react-router": "^1.139.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@mantine/core": "^8.3.12",
    "@mantine/dates": "^8.3.12",
    "@mantine/modals": "^8.3.12",
    "@mantine/notifications": "^8.3.12",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-query": "^5.90.18",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router-devtools": "^1.150.0",
    "dayjs": "^1.11.19",
    "ky": "^1.14.2",
    "liquidjs": "^10.24.0",
    "rehype": "^13.0.2",
    "rehype-sanitize": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-router": "^1.150.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-mantine": "^4.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-dom": "^2.7.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-react-x": "^2.7.1",
    "globals": "^17.0.0",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.8.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1"
  }
